{
  "dashboard": {
    "title": "Agentic AI Platform - Agent Performance",
    "tags": ["agentic-ai", "agents", "performance"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Agents by Execution Count",
        "type": "bargauge",
        "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent_name) (agent_executions_total)",
            "legendFormat": "{{agent_name}}"
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        }
      },
      {
        "id": 2,
        "title": "Agent Success vs Failure Rate",
        "type": "graph",
        "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent_name) (rate(agent_executions_total{status=\"completed\"}[10m]))",
            "legendFormat": "{{agent_name}} Success"
          },
          {
            "expr": "sum by (agent_name) (rate(agent_executions_total{status=\"failed\"}[10m]))",
            "legendFormat": "{{agent_name}} Failed"
          }
        ],
        "yaxes": [
          {"format": "reqps", "label": "Executions/sec"},
          {"format": "short"}
        ]
      },
      {
        "id": 3,
        "title": "Agent Execution Duration Heatmap",
        "type": "heatmap",
        "gridPos": {"x": 0, "y": 8, "w": 24, "h": 8},
        "targets": [
          {
            "expr": "sum(rate(agent_execution_duration_seconds_bucket[5m])) by (le, agent_name)",
            "legendFormat": "{{agent_name}}",
            "format": "heatmap"
          }
        ],
        "dataFormat": "tsbuckets",
        "yAxis": {
          "format": "s"
        }
      },
      {
        "id": 4,
        "title": "Top 10 Slowest Agent Executions",
        "type": "table",
        "gridPos": {"x": 0, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "topk(10, max_over_time(agent_execution_duration_seconds[30m]))",
            "legendFormat": "{{agent_id}} - {{agent_name}}",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {
                "agent_id": 0,
                "agent_name": 1,
                "Value": 2
              },
              "renameByName": {
                "agent_id": "Agent ID",
                "agent_name": "Agent Name",
                "Value": "Duration (s)"
              }
            }
          }
        ]
      },
      {
        "id": 5,
        "title": "Agent Error Distribution",
        "type": "piechart",
        "gridPos": {"x": 12, "y": 16, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent_name) (agent_executions_total{status=\"failed\"})",
            "legendFormat": "{{agent_name}}"
          }
        ]
      },
      {
        "id": 6,
        "title": "Average Execution Time by Agent (Last 24h)",
        "type": "stat",
        "gridPos": {"x": 0, "y": 24, "w": 24, "h": 4},
        "targets": [
          {
            "expr": "avg by (agent_name) (rate(agent_execution_duration_seconds_sum[24h]) / rate(agent_execution_duration_seconds_count[24h]))",
            "legendFormat": "{{agent_name}}"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "orientation": "auto",
          "textMode": "value_and_name",
          "colorMode": "value"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 7,
        "title": "Agent Throughput (Executions per Minute)",
        "type": "graph",
        "gridPos": {"x": 0, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "sum by (agent_name) (rate(agent_executions_total[1m])) * 60",
            "legendFormat": "{{agent_name}}"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Executions/min"},
          {"format": "short"}
        ]
      },
      {
        "id": 8,
        "title": "Agent Resource Utilization",
        "type": "graph",
        "gridPos": {"x": 12, "y": 28, "w": 12, "h": 8},
        "targets": [
          {
            "expr": "avg by (agent_name) (agent_execution_duration_seconds) * sum by (agent_name) (rate(agent_executions_total[5m]))",
            "legendFormat": "{{agent_name}} CPU-seconds"
          }
        ],
        "yaxes": [
          {"format": "short", "label": "Resource Usage"},
          {"format": "short"}
        ]
      }
    ],
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1
  }
}
